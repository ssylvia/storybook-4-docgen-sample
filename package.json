{
  "name": "babel-typescript-sample",
  "version": "0.7.0",
  "license": "MIT",
  "scripts": {
    "storybook": "start-storybook -p 9002 -c .storybook",
    "type-check": "tsc --noEmit",
    "build": "npm run build-types && npm run build-js",
    "build-types": "tsc --emitDeclarationOnly",
    "build-js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.0.0",
    "@storybook/addon-a11y": "^4.0.0-alpha.18",
    "@storybook/addon-actions": "^4.0.0-alpha.18",
    "@storybook/addon-info": "^4.0.0-alpha.18",
    "@storybook/addon-knobs": "^4.0.0-alpha.18",
    "@storybook/addon-links": "^4.0.0-alpha.18",
    "@storybook/addon-options": "^4.0.0-alpha.18",
    "@storybook/addon-storyshots": "^4.0.0-alpha.18",
    "@storybook/addon-viewport": "^4.0.0-alpha.18",
    "@storybook/addons": "^4.0.0-alpha.18",
    "@storybook/react": "^4.0.0-alpha.18",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__addon-info": "^3.4.1",
    "@types/storybook__addon-knobs": "^3.4.1",
    "@types/storybook__addon-links": "^3.3.2",
    "@types/storybook__addon-storyshots": "^3.4.3",
    "@types/storybook__react": "file:types/storybook__react",
    "@types/styled-jsx": "^2.2.5",
    "babel-loader": "^8.0.0",
    "react-docgen-typescript-loader": "^3.0.0-rc.0",
    "styled-jsx": "^3.0.2",
    "typescript": "^3.0.1"
  },
  "dependencies": {
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "moment": "^2.22.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  }
}
